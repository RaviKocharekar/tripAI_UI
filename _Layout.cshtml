    <div class="modal fade" id="filter" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Filter</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                @if (Config.Session.UserFilter != null)
                {
                    <div class="modal-body">
                        <form>
                            <div class="form-group">
                                <label>Country</label>
                                <select class="form-control globalFilter" id="globalFilter_Country">
                                    <option>All</option>
                                    @foreach (var item in Config?.Session?.UserFilter?.Countries)
                                    {
                                        <option value="@item.Id">@item.Name</option>
                                    }
                                </select>
                            </div>
                            <div class="form-group">
                                <label>Customer</label>
                                <select class="form-control globalFilter" id="globalFilter_Customer">
                                    <option>All</option>
                                    @foreach (var item in Config?.Session?.UserFilter?.Customers)
                                    {
                                        <option value="@item.Id">@item.Name</option>
                                    }
                                </select>
                            </div>
                            <div class="form-group">
                                <label>Branches</label>
                                <select class="form-control globalFilter" id="globalFilter_Branches">
                                    <option>All</option>
                                    @foreach (var item in Config?.Session?.UserFilter?.Branches)
                                    {
                                        <option value="@item.Id">@item.Name</option>
                                    }
                                </select>
                            </div>
                            <div class="form-group">
                                <label>Staffs</label>
                                <select class="form-control globalFilter" id="globalFilter_Staffs">
                                    <option>All</option>
                                    @foreach (var item in Config?.Session?.UserFilter?.Staffs)
                                    {
                                        <option value="@item.Id">@item.Name</option>
                                    }
                                </select>
                            </div>
                        </form>
                    </div>
                }
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary">Save changes</button>
                </div>
            </div>
        </div>
    </div>
