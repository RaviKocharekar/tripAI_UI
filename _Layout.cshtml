    <script>
        var _root;
        var handleAJaxError;
        $(function () {
            _root = $("#_root").val();
            handleAJaxError = function () { }
            $(".globalFilter").change(function () {
                var $this = $(this);
                var selectedValue =
                    $.post(_root + "Staff/GetUserFilterData",
                        {
                            countryId: $("#globalFilter_Country").val(),
                            customerId: $("#globalFilter_Customer").val(),
                            branchId: $("#globalFilter_Branches").val(),
                            staffId: $("#globalFilter_Staffs").val()
                        },
                        function (r) {
                            if (r.success) {
                                $("#globalFilter_Country").data("val", $("#globalFilter_Country").val()).empty().append($("<option/>").text("All"));
                                $("#globalFilter_Customer").data("val", $("#globalFilter_Customer").val()).empty().append($("<option/>").text("All"));
                                $("#globalFilter_Branches").data("val", $("#globalFilter_Branches").val()).empty().append($("<option/>").text("All"));
                                $("#globalFilter_Staffs").data("val", $("#globalFilter_Staffs").val()).empty().append($("<option/>").text("All"));
                                $.each(r.data.countries, function () {
                                    $("#globalFilter_Country").append($("<option/>").text(this.name).val(this.id));
                                });
                                $.each(r.data.customers, function () {
                                    $("#globalFilter_Customer").append($("<option/>").text(this.name).val(this.id));
                                });
                                $.each(r.data.branches, function () {
                                    $("#globalFilter_Branches").append($("<option/>").text(this.name).val(this.id));
                                });
                                $.each(r.data.staffs, function () {
                                    $("#globalFilter_Staffs").append($("<option/>").text(this.name).val(this.id));
                                });
                                $("#globalFilter_Country").val($("#globalFilter_Country").data("val"));

                                $("#globalFilter_Customer option[value='" + $("#globalFilter_Customer").data("val") + "']").attr("selected", true)
                                $("#globalFilter_Branches").val($("#globalFilter_Branches").data("val"));
                                $("#globalFilter_Staffs").val($("#globalFilter_Staffs").data("val"));
                            }
                        });
            });
        });

    </script>
    @section Scripts
        {
        <script src="~/lib/jquery/src/Staff/Login.js"></script>
