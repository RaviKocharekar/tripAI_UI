                                            <input type="checkbox" class="filled-in" checked="checked" />
                                            <span>Filled in</span>
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div class="form-row align-items-center">
                                <div class="col-xs-12 col-sm-6 col-md-2 col-lg-2 my-1">
                                    <label class="sr-only" for="flightDeparture">Name</label>
                                    <input id="origin" min="3" required autocomplete="off" name="flightDeparture" type="text" value="@Model.SearchRequest.FromAirport" class="validate autocomplete form-control" onkeydown="ShowHideDiv(false);" onkeyup="loadAirport(event, 'divFlightDepartureModal', this);">
                                    <div class="autocomplete" id="divFlightDepartureModal" style="display: none;"></div>
                                </div>
                                <div class="col-xs-12 col-sm-6 col-md-2 col-lg-2 my-1">
                                    <label class="sr-only" for="flightArrival">Name</label>
                                    <input id="destination" min="3" required autocomplete="off" name="flightArrival" type="text" value="@Model.SearchRequest.ToAirport" class="validate autocomplete form-control" onkeydown="ShowHideDiv(false);" onkeyup="loadAirport(event, 'divFlightArrivalModal', this);">
                                    <div class="autocomplete" id="divFlightArrivalModal" style="display: none;"></div>
                                </div>
                                <div class="col-xs-12 col-sm-6 col-md-2 col-lg-2 my-1 datePickerWrap">
                                    <label class="sr-only" for="flightDepartureDate">Name</label>
                                    <input type="text" name="flightDepartureDate" value=" @(Convert.ToDateTime(Model.SearchRequest.DepartureDate)) " class="datepicker datePickerStyle form-control" id="dateFrom placeholder="Departure Date">
                                </div>
                                <div class="col-xs-12 col-sm-6 col-md-2 col-lg-2 my-1 datePickerWrap">
                                    <label class="sr-only" for="flightReturnDate">Name</label>
                                    <input type="text" name="flightReturnDate" value=" @(string.IsNullOrEmpty(Model.SearchRequest.ReturnDate) ? Convert.ToDateTime(Model.SearchRequest.DepartureDate) : @Convert.ToDateTime(Model.SearchRequest.ReturnDate))" class="datepicker datePickerStyle form-control customDateWrapreturn" id="dateTo" placeholder="Return Date">
                                </div>
                                <div class="col-xs-12 col-sm-6 col-md-2 col-lg-2 my-1">
                                    <div class="dropdown">
                                        <a data-dropup-auto="false" class="btn btn-default dropdown-toggle border form-control" href="#" role="input" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                            <span class="passengercount">@(Model.SearchRequest.Adults + Model.SearchRequest.Childs + Model.SearchRequest.Infants)</span> Traveler <span class="classSelected">@Model.SearchRequest.JourneyClass</span>
                                        </a>
                                        <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                                            <div class="dropdown-item">
                                                <span href="javascript:void(0)" class="leftLink"><span class="adultVal" name="adults">@Model.SearchRequest.Adults</span> Adult</span>
                                                <input type="hidden" class="adultVal" value="@Model.SearchRequest.Adults" name="adults" />
                                                <span href="javascript:void(0)" class="rightLink">
                                                    <div href="#" class="minus adultminus">-</div><div href="#" class="plus adultPlus">+</div>
                                                </span>
                                                <div class="clearfix"></div>
                                            </div>
                                            <div class="dropdown-item">
                                                <span href="javascript:void(0)" class="leftLink"><span class="childVal" name="child">@Model.SearchRequest.Childs</span> Child</span>
                                                <input type="hidden" class="childVal" value="@Model.SearchRequest.Childs" name="child" />
                                                <span href="javascript:void(0)" class="rightLink">
                                                    <div href="#" class="minus childMinus">-</div><div href="#" class="plus childPlus">+</div>
                                                </span>
                                                <div class="clearfix"></div>
                                            </div>
                                            <div class="dropdown-item">
                                                <span href="javascript:void(0)" class="leftLink"><span class="infantVal" name="infant">@Model.SearchRequest.Infants</span> infant <br /><small class="font12">below 2 years</small></span>
                                                <span href="javascript:void(0)" class="rightLink">
                                                    <input type="hidden" class="infantVal" value="@Model.SearchRequest.Infants" name="infant" />
                                                    <div href="#" class="minus infantMinus">-</div><div href="#" class="plus infantPlus">+</div>
                                                </span>
                                                <div class="clearfix"></div>
                                            </div>
                                            <div class="dropdown-divider"></div>
                                            <div class="dropdown-item">
                                                <div class="radioWrap">
                                                    <label>
                                                        <input class="with-gap radioClick" name="class" type="radio" value="Economy" checked="checked" />
                                                        <span>Economy</span>
                                                    </label>
                                                </div>
                                            </div>
                                            <div class="dropdown-item">
                                                <div class="radioWrap">
                                                    <label>
                                                        <input class="with-gap radioClick" name="class" type="radio" value="Premium Economy" />
                                                        <span>Premium Economy</span>
                                                    </label>
                                                </div>
                                            </div>
                                            <div class="dropdown-item">
                                                <div class="radioWrap">
                                                    <label>
                                                        <input class="with-gap radioClick" name="class" type="radio" value="Business" />
                                                        <span>Business</span>
                                                    </label>
                                                </div>
                                            </div>
                                            <div class="dropdown-item">
                                                <div class="radioWrap">
                                                    <label>
                                                        <input class="with-gap radioClick" name="tripType" type="radio" value="First" />
                                                        <span>First</span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-xs-12 col-sm-6 col-md-2 col-lg-2 my-1">
                                    <button type="submit" class="btn btn-primary btn-block border-0"><span class="fa fa-search"></span> Modify Search</button>
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>

