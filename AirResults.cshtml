<style>
    .flt_hide {
        display: none;
    }

    .modifySearchBtn {
        outline: none;
        box-shadow: none;
        border: none;
        height: auto;
        display: flex;
        padding: 10px;
        align-items: center;
        justify-content: center;
        color: #fff;
        line-height: 20px;
    }

        .modifySearchBtn > img {
            margin-right: 5px;
        }

    .swapIcon {
        right: -17px;
    }

    @@media screen and (min-width: 300px) and (max-width: 767px) {
        .radioWrap,
        .checkWrap {
            padding: 10px 0;
        }

            .radioWrap label,
            .checkWrap label {
                /* color: #ffffff; */
            }

            .radioWrap [type="radio"]:not(:checked) + span::before,
            .radioWrap [type="radio"]:not(:checked) + span::after {
                border: 2px solid #ffffff;
            }

            .radioWrap input[type="radio"] {
                top: 15px;
                right: -13px;
            }

            .radioWrap [type="radio"]:not(:checked) + span,
            .radioWrap [type="radio"]:checked + span {
                z-index: 1;
            }

        .listingLeft {
            padding-bottom: 15px;
            margin-bottom: 15px;
        }

            .listingLeft .listingLeftPad {
                border-bottom: 1px solid #9e9e9e;
            }

        .swapIcon {
            display: inline-block;
            transform: rotate(90deg) translate(20px, -150px);
            margin-left: 0;
        }

        .customDateWrap,
        .customDateWrapreturn,
        .customDrop {
            text-align: left;
            /* padding-left: 15px; */
        }

        .modifySearchBtn {
            width: 100%;
            padding: 15px;
        }

        .timeCard {
            padding: 0;
        }

        .timeCardBorder {
            border: 1px solid #9e9e9e;
            border-right: 1px solid #9e9e9e;
            border-bottom: none;
        }

            .timeCardBorder:last-child {
                border-bottom: 1px solid #9e9e9e;
            }

        .tblMrgnBtm {
            height: auto;
        }
    }
</style>
