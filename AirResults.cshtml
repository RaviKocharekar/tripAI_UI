                                    <input type="text" name="flightReturnDate" value=" @(string.IsNullOrEmpty(Model.SearchRequest.ReturnDate) ? Convert.ToDateTime(Model.SearchRequest.DepartureDate) : @Convert.ToDateTime(Model.SearchRequest.ReturnDate))" class="datepicker datePickerStyle form-control customDateWrapreturn" id="dateTo" placeholder="Return Date">
                                </div>
                                <div class="col-xs-12 col-sm-6 col-md-2 col-lg-2 my-1">
                                    <div class="dropdown">
                                        <a data-dropup-auto="false" class="btn btn-default dropdown-toggle border form-control" href="#" role="input" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                            <span class="passengercount">@(Model.SearchRequest.Adults + Model.SearchRequest.Childs + Model.SearchRequest.Infants)</span> Traveler <span class="classSelected">@Model.SearchRequest.JourneyClass</span>
                                        </a>
                                        <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                                            <div class="dropdown-item">
                                                <span href="javascript:void(0)" class="leftLink"><span class="adultVal" name="adults">@Model.SearchRequest.Adults</span> Adult</span>
                                                <input type="hidden" class="adultVal" value="@Model.SearchRequest.Adults" name="adults" />
                                                <span href="javascript:void(0)" class="rightLink">
                                                    <div href="#" class="minus adultminus">-</div><div href="#" class="plus adultPlus">+</div>
                                                </span>
                                                <div class="clearfix"></div>
                                            </div>
                                            <div class="dropdown-item">
                                                <span href="javascript:void(0)" class="leftLink"><span class="childVal" name="child">@Model.SearchRequest.Childs</span> Child</span>
                                                <input type="hidden" class="childVal" value="@Model.SearchRequest.Childs" name="child" />
                                                <span href="javascript:void(0)" class="rightLink">
                                                    <div href="#" class="minus childMinus">-</div><div href="#" class="plus childPlus">+</div>
                                                </span>
                                                <div class="clearfix"></div>
                                            </div>
                                            <div class="dropdown-item">
                                                <span href="javascript:void(0)" class="leftLink"><span class="infantVal" name="infant">@Model.SearchRequest.Infants</span> infant <br /><small class="font12">below 2 years</small></span>
