                                    <input id="destination" min="3" required autocomplete="off" name="flightArrival" type="text" value="@Model.SearchRequest.ToAirport" class="validate autocomplete form-control" onkeydown="ShowHideDiv(false);" onkeyup="loadAirport(event, 'divFlightArrival', this);">
